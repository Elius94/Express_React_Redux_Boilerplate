{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-multi-spaces": [ "error", {
            "ignoreEOLComments": true
        }],
        "indent": ["error", 4]
    }
}
